!URxvt*termName:		xterm
!URxvt*urlLauncher:      /usr/bin/iceweasel
! matcher.button      # 3 is a right-click
URxvt*matcher.button:	3
URxvt*saveLines:	8192
URxvt.perl-ext-common:	default
URxvt*fading: 		5
URxvt*fadeColor:	#222222
URxvt*buffered:		true
!URxvt.font: xft:Bitstream Vera Sans Mono-9.5
URxvt*visualBell:	True
URxvt*cursorColor:	#d8d8d8
