XBiff*foreground: 	white
XBiff*onceOnly:		true
XBiff*flip:		false
! use xft for drawing fonts
! URxvt.font: xft:Bitstream Vera Sans Mono-8.5
URxvt*transparent:	true
URxvt.background: 	[50]black
URxvt*shading:	        40	
